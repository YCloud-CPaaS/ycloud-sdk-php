{
    "name": "ycloud/ycloud-sdk-php",
    "description": "YCloud SDK for PHP",
    "keywords": [
        "ycloud",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://ycloud.com",
    "license": "MIT",
    "authors": [
        {
            "name": "YCloud <service@ycloud.com>",
            "homepage": "https://ycloud.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "YCloud\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "YCloud\\Client\\Test\\" : "test/" }
    }
}
